#import <stdio.h>
#include "List.h"

int main (void) {

  int size = 0;
  int e = 0;
 
  printf("How many elements? \n");
  scanf("%d", &size);

  for (int i = 0; i < size; i++){   // Insertion
    printf("Enter element: ");
    scanf("%d", &e);
    insert(&l, e);  
  }
  
  print(&l);                        // Printing

  clear(&l);                        // Deallocating

    return 0;
}
